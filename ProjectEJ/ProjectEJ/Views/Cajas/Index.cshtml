@model IEnumerable<ProjectEJ.Models.Caja>

@{
    ViewBag.Title = "Caja inicial";
}

<h2>Caja inicial</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.ActionLink("Actualizar monto", "Edit", new { id=item.Id }) |
    
        </td>
    </tr>
}

</table>
